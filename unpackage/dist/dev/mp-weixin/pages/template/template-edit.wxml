<view class="container"><view><view style="height:30rpx;"></view><view class="panel panel-one"><view class="uni-form-item uni-row"><view class="title">行业名称</view><input class="right-content uni-input" name="name" placeholder="请输入行业名称" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view></view><block wx:if="{{imgUrl==''}}"><view data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" class="panel-img" bindtap="__e"><view class="img-add">+</view></view></block><block wx:else><view data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" class="template-img" bindtap="__e"><image class="img" src="{{imgUrl}}"></image></view></block></view><view class="footer"><view class="btn"><button class="cancel" type="default" data-event-opts="{{[['tap',[['submit',[0]]]]]}}" bindtap="__e">保存</button><button class="submit" type="default" data-event-opts="{{[['tap',[['submit',[1]]]]]}}" bindtap="__e">设计模板</button></view></view></view>