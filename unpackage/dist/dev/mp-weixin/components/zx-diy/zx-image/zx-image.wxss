    vertical-align: bottom;
    max-height: 100%;
    height: auto;
    width: auto;
}
.modify-image{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 80px;
    height: 20px;
    line-height: 20px;
    font-size: 12px;
    color: #fff;
    background: rgba(0,0,0,.5);
